public class EulerFormula {
    public EulerFormula() {
    }

    public static void main(String[] args) {
        System.out.println(eulerFormula());
    }

    public static double eulerFormula() {
        double i = Math.sqrt(-1.0);
        double pi = Math.PI;
        double e = Math.E;
        double power = i * pi;
        return Math.pow(e, power) + 1.0;
    }
}
